$s1 = [
    "host" => "hexlet.io",
    "timeout" => 50,
    "proxy" => "123.234.53.22",
    "config" => [
        'debug' => 'none'
    ]
];

$s2 = [
    "host" => "hexlet.io",
    "proxy" => "123.234.53.55",
    "config" => [
        'debug' => 'none',
        'test'
    ]
];

$d1 = genDiff('./tests/fixtures/file1.json', './tests/fixtures/file2.json');

